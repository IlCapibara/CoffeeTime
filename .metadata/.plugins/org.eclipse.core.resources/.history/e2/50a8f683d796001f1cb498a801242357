package com.gc.cleaner;

import java.io.File;
import java.util.List;

import com.gc.cleaner.model.PomInfo;
import com.gc.cleaner.service.RepositoryService;
import com.gc.cleaner.service.RepositoryServiceImpl;

public class SeeYouTomorrowMain {
	
	static RepositoryService rs = new RepositoryServiceImpl();
	
    public static void main(String[] args) {
    	
    	List<File> pomFiles = rs.findAllPomFiles("C://rgi/projects/uma");
    	PomInfo pomInfo;
    	
		for(File pomFile : pomFiles) {
    		pomInfo = rs.readPomFile(pomFile);
    	}        
    }
}
